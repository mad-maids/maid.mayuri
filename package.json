{
  "name": "maid.mayuri",
  "version": "1.0.0",
  "main": "dist/app.js",
  "private": true,
  "description": "Discord bot written in TypeScript for Mad Maids",
  "repository": "https://github.com/mad-maids/maid.mayuri",
  "author": "Mad Maids (https://maid.uz)",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --check \"src/**/*.ts\"",
    "start:dev": "nodemon dist/app.js",
    "build:dev": "tsc -w",
    "lint:dev": "yarn lint --fix",
    "format:dev": "yarn format --write",
    "concurrent": "concurrently \"yarn build:dev\" \"yarn start:dev\"",
    "production": "yarn build && yarn start"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.7.5",
    "chalk": "^4.1.2",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "youtube-sr": "^4.1.9",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "_moduleAliases": {
    "@src": "dist",
    "@core": "dist/core",
    "@delta": "dist/delta",
    "@model": "dist/model",
    "@utils": "dist/utils"
  }
}
